{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "PublicReadGetObject",
            "Principal": {
                "AWS": "arn"
            },
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket",
                "s3:PutObject"
                ],
            "Resource": [
                "${bucket_arn}/*",
                "${bucket_arn}"
                ]
        }
    ]
}